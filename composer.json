{
    "name": "eclipxe/xlsxexporter",
    "description": "PHP Office Open XML Spreadsheet (xlsx) exporter",
    "keywords": ["excel", "xlsx", "office-open-xml"],
    "homepage": "https://github.com/eclipxe13/XLSXExporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "jakub-onderka/php-parallel-lint": "@stable",
        "satooshi/php-coveralls": "@stable",
        "scrutinizer/ocular": "@stable"
    },
    "autoload": {
        "psr-4": {
            "XLSXExporter\\": "sources/XLSXExporter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XLSXExporterTests\\": "tests/XLSXExporterTests/"
        }
    }
}
