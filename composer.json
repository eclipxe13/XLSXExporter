{
    "name": "eclipxe/xlsxexporter",
    "description": "PHP Office Open XML Spreadsheet (xlsx) exporter",
    "keywords": ["excel", "xlsx", "office-open-xml"],
    "homepage": "https://github.com/eclipxe13/XLSXExporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-zip": "*",
        "eclipxe/engineworks-progress-status": "^1.0"
    },
    "suggest": {
        "eclipxe/engineworks-dbal": "Export recordsets as worksheets in a workbook"
    },
    "require-dev": {
        "eclipxe/engineworks-dbal": "^1.6",
        "phpunit/phpunit": "^6.1",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.3.2"
    },
    "autoload": {
        "psr-4": {
            "XLSXExporter\\": "src/XLSXExporter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XLSXExporterTests\\": "tests/XLSXExporterTests/"
        }
    }
}
